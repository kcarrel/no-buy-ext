{
 "manifest_version": 2,
 "name": "No Buy",
 "version": "1.0.0",
 "description": "Reminds users at checkout that they are on a no buy.",
 "permissions": ["activeTab", "declarativeContent", "storage", "<all_urls>"],
 "browser_action": {
   "default_title": "No Buy",
   "default_popup": "popup.html",
   "default_icon": "icon.png"
 }
}